<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="cl.bootcamp.ind_12.FirstFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" >
        <action
            android:id="@+id/action_firstFragment_to_secondFragment"
            app:destination="@id/secondFragment" />
    </fragment>
    <fragment
        android:id="@+id/secondFragment"
        android:name="cl.bootcamp.ind_12.SecondFragment"
        android:label="fragment_second"
        tools:layout="@layout/fragment_second" >
        <action
            android:id="@+id/action_secondFragment_to_sakuragiFragment"
            app:destination="@id/sakuragiFragment" />
        <action
            android:id="@+id/action_secondFragment_to_ryotaFragment"
            app:destination="@id/ryotaFragment" />
        <action
            android:id="@+id/action_secondFragment_to_akagiFragment2"
            app:destination="@id/akagiFragment2" />
        <action
            android:id="@+id/action_secondFragment_to_mitsuiFragment"
            app:destination="@id/mitsuiFragment" />
        <action
            android:id="@+id/action_secondFragment_to_rukawaFragment"
            app:destination="@id/rukawaFragment"

            />
    </fragment>
    <fragment
        android:id="@+id/akagiFragment2"
        android:name="cl.bootcamp.ind_12.AkagiFragment"
        android:label="fragment_akagi"
        tools:layout="@layout/fragment_akagi" >
        <action
            android:id="@+id/action_akagiFragment2_to_videoAkagiFragment"
            app:destination="@id/videoAkagiFragment" />
        <action
            android:id="@+id/action_akagiFragment2_to_firstFragment"
            app:destination="@id/firstFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right"
            />
    </fragment>
